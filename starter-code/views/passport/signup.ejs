<h2> Sign Up </h2>

<% if (typeof errorMessage !== "undefined") { %>
  <div class="error"> <%= errorMessage %> </div>
<% } %>

<form method="post" action="/signup">
  <label for="signupUsername"> Your Name </label>
  <input  id="signupUsername" type="text" name="signupUsername">
  <br>

  

  <label for="signupPassword-input"> Password </label>
  <input  id="signupPassword-input" type="password" name="signupPassword">
  <br>

  <button> Sign Up </button>
</form>

<p>
  Do you already have an account?
  <a href="/login"> Log In </a>
</p>
